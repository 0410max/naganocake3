<!DOCTYPE html>
<html>
  <head>
    <title>NaganoCake</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <header>
      <ul class='not-login'>
        <%= link_to root_path do %>
          <li class='not-login-logo'>LOGO</li>
        <% end %>
        <a href="#">
          <li class='not-login-item-index'>商品一覧</li>
        </a>
        <%= link_to new_end_user_session_path do %>
          <li class='not-login-login'>ログイン</li>
        <% end %>
        <%= link_to new_end_user_registration_path do %>
          <li class='not-login-signup'>新規登録</li>
        <% end %>
      </ul>
    </header>
    <%= yield %>
  </body>
</html>
